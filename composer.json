{
    "name": "accentinteractive/disallowlister",
    "description": "Keep your laravel logs small and tidy.",
    "keywords": [
        "accentinteractive",
        "disallowlister"
    ],
    "homepage": "https://github.com/accentinteractive/disallowlister",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joost van Veen",
            "email": "joost@accentinteractive.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "4.*|5.*|6.*"
    },
    "autoload": {
        "psr-4": {
            "Accentinteractive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accentinteractive\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
